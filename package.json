{
  "name": "vein-detection-system",
  "version": "1.0.0",
  "description": "静脉检测系统 - 完整的测试框架和集成方案",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "cd frontend/ultrasound-vein-detection && pnpm dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "node tests/e2e/e2e-test-runner.js",
    "test:e2e:setup": "npm run build && ./scripts/deploy.sh start",
    "test:e2e:report": "jest tests/e2e --coverage",
    "test:performance": "jest tests/performance",
    "test:benchmark": "jest tests/performance --runInBand",
    "test:memory": "node --expose-gc tests/performance/memory-test.js",
    "test:cpu": "node tests/performance/cpu-test.js",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests",
    "type-check": "tsc --noEmit",
    "security": "npm audit && snyk test",
    "deploy": "./scripts/deploy.sh deploy",
    "deploy:check": "./scripts/deploy.sh check",
    "deploy:start": "./scripts/deploy.sh start",
    "deploy:stop": "./scripts/deploy.sh stop",
    "deploy:restart": "./scripts/deploy.sh restart",
    "deploy:redeploy": "./scripts/deploy.sh redeploy",
    "deploy:monitor": "./scripts/deploy.sh monitor",
    "deploy:backup": "./scripts/deploy.sh backup",
    "deploy:cleanup": "./scripts/deploy.sh cleanup",
    "logs": "./scripts/deploy.sh logs",
    "monitor": "node scripts/monitor.js",
    "migrate": "./scripts/deploy.sh migrate",
    "health-check": "curl -f http://localhost:8000/health",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../video-service && npm install",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules video-service/node_modules",
    "clean:cache": "rm -rf .jest-cache coverage test-results",
    "clean:logs": "rm -rf logs/*",
    "clean:temp": "rm -rf temp/* uploads/*",
    "doc:generate": "jsdoc -c jsdoc.conf.json",
    "doc:serve": "jsdoc -c jsdoc.conf.json && open http://localhost:8080",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "prepush": "npm run test && npm run build",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/vein-detection-system.git"
  },
  "keywords": [
    "vein-detection",
    "medical-imaging",
    "computer-vision",
    "deep-learning",
    "machine-learning",
    "testing",
    "integration",
    "performance-optimization",
    "monitoring"
  ],
  "author": {
    "name": "Vein Detection System Team",
    "email": "dev@vein-detection.com",
    "url": "https://vein-detection.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-org/vein-detection-system/issues"
  },
  "homepage": "https://github.com/your-org/vein-detection-system#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "./config/test/jest.config.js"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "no-console": "warn"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "ignorePatterns": [
      "node_modules/",
      "dist/",
      "build/",
      "coverage/",
      "*.min.js"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test && npm run build"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "puppeteer": "^21.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "multer": "1.4.4",
    "sharp": "^0.32.6",
    "axios": "^1.5.0",
    "socket.io": "^4.7.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "joi": "^17.10.1",
    "rate-limiter-flexible": "^3.0.4",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "validator": "^13.11.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "bundleDependencies": [
    "sharp"
  ],
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
